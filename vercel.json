{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    },
    {
      "src": "Dockerfile",
      "use": "@vercel/docker"
    }
  ],
  "routes": [
    {
      "src": "/python/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/docker/(.*)",
      "dest": "/"
    }
  ]
}
